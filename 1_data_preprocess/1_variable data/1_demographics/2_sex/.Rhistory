knitr::opts_chunk$set(echo = TRUE)
# Import package
library(dplyr)
library(readr)
# Check invalid lines for head and tail
all_lines <- readLines("sex_by_sa2_raw_2021.csv")
length(all_lines)
head(all_lines, 11)
tail(all_lines, 9)
# Remove invalid lines
clean_lines <- all_lines[11:(length(all_lines) - 9)]
# define temporary file name
temp_file <- "sex_by_sa2_temp_2021.csv"
# Save temporary file for document
writeLines(clean_lines, temp_file)
# read temporary file
sex_2021 <- read_csv(temp_file)
# Display data frame
head(sex_2021)
# Remove NA column
sex_2021 <- sex_2021 %>% select(where(~ !all(is.na(.))))
# Remove 'Total' column
sex_2021 <- sex_2021 %>% select(-Total)
# Remove 'SA2 (UR)' row
sex_2021 <- sex_2021 %>% filter(.[[1]] != "SA2 (UR)")
# Rename sa2
sex_2021 <- sex_2021 %>% rename(sa2 = 'SEXP Sex')
# Check data frame
head(sex_2021)
# Read wslhd sa2 file
wslhd_sa2_2021 <- read_csv('wslhd_sa2_2021.csv')
# Filter wslhd data for sex_2021
sex_wslhd_2021 <- sex_2021 %>%
semi_join(wslhd_sa2_2021, by = "sa2")
# Check data frame
head(sex_wslhd_2021)
nrow(sex_wslhd_2021)
# Save wslhd sex_2021 for document
write_csv(sex_wslhd_2021, "sex_wslhd_2021.csv")
